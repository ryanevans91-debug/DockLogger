{
	"name": "docklogger",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"preview": "vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"cap:sync": "npm run build && npx cap sync",
		"cap:android": "npm run build && npx cap sync android && npx cap open android",
		"cap:ios": "npm run build && npx cap sync ios && npx cap open ios",
		"cap:assets": "npx @capacitor/assets generate --android --ios"
	},
	"devDependencies": {
		"@capacitor/assets": "^3.0.5",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/adapter-static": "^3.0.10",
		"@sveltejs/kit": "^2.49.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/postcss": "^4.1.18",
		"autoprefixer": "^10.4.23",
		"postcss": "^8.5.6",
		"svelte": "^5.45.6",
		"svelte-check": "^4.3.4",
		"tailwindcss": "^4.1.18",
		"typescript": "^5.9.3",
		"vite": "^7.2.6"
	},
	"dependencies": {
		"@capacitor-community/sqlite": "^7.0.3",
		"@capacitor/android": "^8.0.1",
		"@capacitor/browser": "^8.0.0",
		"@capacitor/camera": "^8.0.0",
		"@capacitor/cli": "^8.0.1",
		"@capacitor/core": "^8.0.1",
		"@capacitor/filesystem": "^8.0.0",
		"@capacitor/ios": "^8.0.1",
		"@capacitor/share": "^8.0.0",
		"jeep-sqlite": "^2.8.0",
		"sql.js": "^1.13.0"
	}
}
